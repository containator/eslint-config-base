    "eslint": "^8.28.0",
    "eslint-plugin-import": "^2.26.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:containator/eslint-config-base.git"
  },
  "scripts": {
    "lint:fix": "eslint . --ext .js --fix",
    "prepare": "husky install"
  },
  "version": "1.0.0"
}
