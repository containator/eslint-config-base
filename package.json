{
  "author": "Maxim A.",
  "devDependencies": {
    "eslint": "^8.11.0",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
