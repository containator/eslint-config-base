{
  "author": "Maxim A.",
  "devDependencies": {
    "eslint": "^8.28.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
