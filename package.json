    "eslint": "^8.11.0",
    "eslint-plugin-import": "^2.25.4"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:containator/eslint-config-base.git"
  },
  "scripts": {
    "lint:fix": "eslint . --ext .js --fix",
    "prepare": "husky install"
  },
  "version": "1.0.0"
}
